	TraceResult tr;

	
	for (int index = 0; index < num_waypoints; index++) {
		
		if (waypoints[index].flags & (1 << 31) || waypoints[index].flags & (1 << 14) ||
			waypoints[index].flags & lostBotIgnoreFlags)
			continue;

		
		if (waypoints[index].flags & (1 << 2) && (waypoints[index].flags & ((1 << 0) + (1 << 1))) != pBot->current_team)
			continue;

		
		
		if (index == pBot->current_wp)
			continue;

		
		Vector distance = waypoints[index].origin - pBot->pEdict->v.origin;
		double distance_squared = distance.x * distance.x + distance.y * distance.y + distance.z * distance.z;

		
		
		if (pBot->pEdict->v.origin.z + 50.0 < waypoints[index].origin.z) {
			if (runnerUp == -1 && distance_squared < 640000.0) 
			{
				
				UTIL_TraceLine(pBot->pEdict->v.origin, waypoints[index].origin, dont_ignore_monsters,
					pBot->pEdict->v.pContainingEntity, &tr);

				
				if (tr.flFraction >= 1.0)
					runnerUp = index;
			}

			continue;
		}

		
		if (distance_squared < min_distance_squared) {
			
			UTIL_TraceLine(pBot->pEdict->v.origin, waypoints[index].origin, dont_ignore_monsters,
				pBot->pEdict->v.pContainingEntity, &tr);

			
			if (tr.flFraction >= 1.0) {
				min_index = index;
				min_distance_squared = distance_squared;
			}
		}
	}

	if (min_index == -1)
		pBot->current_wp = runnerUp;
	else
		pBot->current_wp = min_index;

	
	pBot->f_current_wp_deadline = pBot->f_think_time + 7.0;

	
	if (pBot->current_wp != -1 && pBot->f_killed_time + 4.0 > pBot->f_think_time) {
		BotUpdateHomeInfo(pBot); 

		
		
		
	}
}

CBaseEntity* UTIL_FindEntityInSphere(CBaseEntity* pStartEntity, const Vector& vecCenter, const float flRadius)
{

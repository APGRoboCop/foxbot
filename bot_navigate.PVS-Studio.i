					pBot->pEdict->v.button |= (1 << 1);
				}
				else {
					const int duckResult = BotShouldDuckUnder(pBot);
					if (duckResult == 2) 
					{
						
						pBot->f_duck_time = pBot->f_think_time + 0.3;
					}
					else 
					{
						
						

template <typename Type> class LIter;

template <typename Type> class List
{
	
	friend class LIter<Type>;

protected:
	struct Node {
		Type element;      
		Node* prev, * next; 
	};

	
	int total;

	
	Node* head, * tail;

public:
	
	
	List()
	{
		head = tail = 0;
		total = 0;
	}

	
	
	~List()
	{
		clear();
		total = 0;
	}

	
	

	
	bool isEmpty()
	{
		return (head == 0);
	}

	
	void addHead(const Type& v);
	void addTail(const Type& v);

	
	
	void insert(const Type& v, LIter<Type>& loc);
	void remove(LIter<Type>& loc);

	
	void clear();

	
	int size();
};




template <typename Type> void List<Type>::addHead(const Type& v)
{
	
	Node* n = new Node;
	n->element = v;
	
	n->prev = 0;

	
	n->next = head;
	if (head != 0)
		head->prev = n;
	head = n;

	total++;

	
	
	if (tail == 0)
		tail = n;
}




template <typename Type> void List<Type>::addTail(const Type& v)
{
	
	Node* n = new Node;
	n->element = v;
	
	n->next = 0;

	
	
	if (tail != 0)
		tail->next = n;
	n->prev = tail;
	tail = n;

	total++;

	
	
	if (head == 0)
		head = n;
}



template <typename Type> void List<Type>::clear()
{
	
	
	while (!isEmpty()) {
		Node* n = head;
		head = head->next;
		delete n;
	}

	total = 0;

	
	
	tail = 0;
}

template <typename Type> int List<Type>::size()
{
	return total;
}



template <typename Type> class LIter
{
	
	friend class List<Type>;

private:
	
	List<Type>* list;
	

	
	typename List<Type>::Node* currentNode;

public:
	
	
	explicit LIter(List<Type>* listToUse): currentNode(nullptr)
	{
		list = listToUse;
	}

	
	
	
	void begin()
	{
		currentNode = list->head;
	}
	void beginReverse()
	{
		currentNode = list->tail;
	}

	
	
	bool end()
	{
		return (currentNode == 0);
	}

	
	
	
	LIter<Type>& operator++()
	{
		if (currentNode != 0)
			currentNode = currentNode->next;
		return *this;
	}

	
	
	
	LIter<Type>& operator--()
	{
		if (currentNode != 0)
			currentNode = currentNode->prev;
		return *this;
	}

	
	
	Type* current()
	{
		if (currentNode == 0)
			return 0;
		return &(currentNode->element);
	}
};





template <typename Type> void List<Type>::insert(const Type& v, LIter<Type>& loc)
{
	
	
	Node* nextNode = loc.currentNode;
	
	
	if (nextNode == 0)
		return;

	
	Node* newNode = new Node;
	newNode->element = v;

	
	
	Node* prevNode = nextNode->prev;

	
	
	if (prevNode != 0)
		prevNode->next = newNode;
	newNode->prev = prevNode;
	
	newNode->next = nextNode;
	nextNode->prev = newNode;

	total++;

	
	
	if (prevNode == 0)
		head = newNode;

	
	loc.currentNode = newNode->next;
}






template <typename Type> void List<Type>::remove(LIter<Type>& loc)
{
	
	
	Node* deletedNode = loc.currentNode;

	
	
	
	Node* leftNode = deletedNode->prev;
	Node* rightNode = deletedNode->next;
	if (leftNode != 0)
		leftNode->next = rightNode;
	if (rightNode != 0)
		rightNode->prev = leftNode;

	
	
	
	if (deletedNode == head)
		head = deletedNode->next;
	if (deletedNode == tail)
		tail = deletedNode->prev;

	total--;

	
	loc.currentNode = deletedNode->next;

	
	delete deletedNode;
}
